    StrSql = " SELECT gti_pol_det_param.polparamnro,gti_pol_det_param.polparamvalor FROM gti_pol_det_param " & _
             " INNER JOIN gti_pol_param ON gti_pol_det_param.polparamnro = gti_pol_param.polparamnro " & _
