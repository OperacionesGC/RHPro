            End If
            I = rs_Pol!CabPolNivel
            
            Arr_PolE(I, J).CabPolNro = rs_Pol!CabPolNro
            Arr_PolE(I, J).CabPolNivel = rs_Pol!CabPolNivel
            If Not EsNulo(rs_Pol!DetPolPrograma) Then
