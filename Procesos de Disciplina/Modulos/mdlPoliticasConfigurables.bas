Dim Reg_Sal As Long
Dim otroDia As Boolean
Dim J As Integer
Dim Weekdia As Byte
Dim objrsDesgl As New ADODB.Recordset
Dim TotHor As Single
Dim TotHorHHMM As String

'FGZ - 30/08/2006
'Dim nro_desg As integer
Dim nro_desg As Long
