                             "horhorahasta,hormanual,horvalido,jusnro,jusnro2,normnro,normnro2,Ternro," & _
                             "thnro,turnro,regent,horrealent,regsal,horrealsal,Empleg,horfecrep,horfecgen) VALUES (" & _
