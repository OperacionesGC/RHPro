                                                   Hasta_Nlb = Aux_hasta
                                        End If '(8)
                                        
                                   End If '(7)
                                
                              Else '(6)
                                  If rs_Gti_Detturtemp!ttemphdesde2 <> "0000" And Not IsNull(rs_Gti_Detturtemp!ttemphdesde2) And _
