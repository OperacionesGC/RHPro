Function ReemplazarCampos(ByVal query As String) As String
    Dim campo As String
    Dim valor
    
    Do Until InStr(1, query, "@@") = 0
        campo = Mid(query, InStr(1, query, "@@") + 2, Len(query))
        campo = Mid(campo, 1, InStr(1, campo, "@@") - 1)
        
        Select Case UCase(campo)
        Case "EMPLEADO":
            valor = Empleado.Ternro
        Case "FECHA":
            valor = ConvFecha(p_fecha)
            'Valor = "'" & Format(p_fecha, "YYYY/MM/DD") & "'"
        Case "LUNES":
            valor = "'Lunes'"
        Case "MARTES":
            valor = "'Martes'"
        Case "MIERCOLES":
            valor = "'Miercoles'"
        Case "JUEVES":
            valor = "'Jueves'"
        Case "VIERNES":
            valor = "'Viernes'"
        Case "SABADO":
            valor = "'Sabado'"
        Case "DOMINGO":
            valor = "'DOMINGO'"
        Case "MONDAY":
            valor = "'MONDAY'"
        Case "TUESDAY":
            valor = "'TUESDAY'"
        Case "WEDNESDAY":
            valor = "'WEDNESDAY'"
        Case "THURSDAY":
            valor = "'THURSDAY'"
        Case "FRIDAY":
            valor = "'Friday'"
        Case "SATURDAY":
            valor = "'Saturday'"
        Case "Sunday":
            valor = "'Sunday'"
        Case Else
        End Select
        query = Replace(query, "@@" & campo & "@@", valor)
        
    Loop
    ReemplazarCampos = query
End Function

