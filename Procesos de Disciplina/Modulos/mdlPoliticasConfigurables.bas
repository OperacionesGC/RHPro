                    Call Cambiar_Horas_Desde2(nro_desg, hora_desde_desg, fecha_desde_desg, Cambio_dia, fecpar1)
                    Call Cambiar_Horas_hasta2(nro_desg, hora_hasta_desg, fecha_hasta_desg, Cambio_dia, fecpar1)
