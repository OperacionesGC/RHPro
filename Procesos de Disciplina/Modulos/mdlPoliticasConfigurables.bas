Dim rs As New ADODB.Recordset


diatipo = 0

If esFeriado And Dia_Libre Then
    Call Politica(380)
    
    If Dia_Libre Then
        diatipo = 1
    Else
        diatipo = 2
    End If
Else
    If esFeriado Then
        diatipo = 2
    Else
        If Dia_Libre Then
            diatipo = 1
        End If
    End If
End If

Weekdia = Weekday(p_fecha)


