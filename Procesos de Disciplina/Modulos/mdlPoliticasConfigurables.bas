Public Sub Politica450_old(Nro_Turno As Long, p_fecha As Date, NroTer As Long, Nro_Grupo As Long, Nro_fpgo As Integer, Dia_Libre As Boolean, esFeriado As Boolean, Existe_Reg As Boolean, deb As Boolean, subn As Integer, ByVal Cabecera As Long, ByVal Detalle As Long)
' --------------------------------------------------------------
' Descripcion: Imputa Horas sin control de presencia.
' Autor: ?
' Ultima modificacion: FGZ - 29/07/2003
' --------------------------------------------------------------

'/*------------------------------------------------------------------------------
'    Busca el d¡a que le corresponde al empleado, de acuerdo al d¡a que se esta
'    procesando y al turno al que pertenece.
'------------------------------------------------------------------------------*/

Dim Weekdia As Integer
Dim diatipo As Integer
Dim J As Integer
Dim Dia As Integer

