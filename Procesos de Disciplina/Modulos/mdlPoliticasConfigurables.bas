                    End If
                End If
                
                If Hay_Reg Then
                    
            
            
                    'Paro       [---------------]
                    'registracion     (----------------)
                    If objFechasHoras.Menor_Hora(ParoFDesde, HCDesde, fecpar1, par1) And objFechasHoras.Menor_Hora(fecpar1, par1, ParoFHasta, ParoHHasta) And objFechasHoras.Menor_Igual_Hora(ParoFHasta, ParoHHasta, fecpar1, par2) Then
                        objFechasHoras.RestaHs fecpar1, par1, ParoFHasta, ParoHHasta, Tdias, Thoras, Tmin
                        HCDesde = par1
                        HCHasta = ParoHHasta
                        TotHor = (Tdias * 24) + (Thoras + (Tmin / 60))
                    End If
        
                    'paro           [---------------]
                    'Horario    (----------------)
                    If objFechasHoras.Mayor_Igual_Hora(ParoFDesde, HCDesde, fecpar1, par1) And objFechasHoras.Menor_Hora(ParoFDesde, HCDesde, fecpar1, par2) And objFechasHoras.Mayor_Hora(ParoFHasta, ParoHHasta, fecpar1, par2) Then
                        objFechasHoras.RestaHs ParoFDesde, HCDesde, fecpar1, par2, Tdias, Thoras, Tmin
