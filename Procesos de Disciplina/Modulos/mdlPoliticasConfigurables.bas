                StrSql = StrSql & ValorNulo & ","
                StrSql = StrSql & ValorNulo & ","
                StrSql = StrSql & ValorNulo & ","
                StrSql = StrSql & Empleado.Ternro & ","
                StrSql = StrSql & NroTh & ","
                StrSql = StrSql & Nro_Turno & ","
                StrSql = StrSql & Reg_Ent & ","
                StrSql = StrSql & "'" & IIf(J = 1, par1, "0000") & "',"
