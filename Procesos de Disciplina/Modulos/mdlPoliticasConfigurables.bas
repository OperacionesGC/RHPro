    Flog.writeline Espacios(Tabulador * 2) & " Antes del control "
    Flog.writeline Espacios(Tabulador * 2) & " DESDE    : " & fecha_desde & " " & hora_desde
    Flog.writeline Espacios(Tabulador * 2) & " HASTA    : " & fecha_hasta & " " & hora_hasta
