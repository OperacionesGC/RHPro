'        Flog.writeline
'    End If
    
    If (esFeriado) Then
        Desde_Nlb = ""
        Hasta_Nlb = ""
        Corte = 2
        Exit Sub
    End If

    If (Not Dia_Libre) And (Not esFeriado) Then '(0)
        Aux_desde = Desde_Lab
        Aux_hasta = Hasta_Lab
    
        If Corte = 2 Then
            Desde_Nlb = Aux_desde
            Hasta_Nlb = Aux_hasta
            Corte = 3
