    StrSql = "SELECT gti_justificacion.*,gti_tipojust.thnro FROM gti_justificacion"
    StrSql = StrSql & " INNER JOIN gti_tipojust ON gti_justificacion.tjusnro = gti_tipojust.tjusnro "
