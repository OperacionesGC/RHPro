                 StrSql = StrSql & " WHERE ternro = " & rs!Ternro
                 StrSql = StrSql & " and thnro = " & NocAut
                 StrSql = StrSql & " and adfecha = " & ConvFecha(p_fecha)
                 OpenRecordset StrSql, rsAD
