                            If THOrigen = 1008 Then Destino = 1036
                            If THOrigen = 1013 Then Destino = 1037
                            If THOrigen = 1014 Then Destino = 1038
                            If THOrigen = 1015 Then Destino = 1044
                            If THOrigen = 5 Then Destino = 1040
