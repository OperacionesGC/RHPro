                                           Desde_Lab = ""
                                           Desde_Nlb = Aux_desde
                                           Hasta_Nlb = Aux_hasta
                                           Corte = 3
                                Else '(7)
                                    
                                    If Hasta_Lab > rs_Gti_Dias!diahorahasta3 And _
                                       Not ((rs_Gti_Dias!diasiguiente = 3 And otroDia)) Then
                                    
                                               Hasta_Lab = rs_Gti_Dias!diahorahasta3
                                               Desde_Nlb = rs_Gti_Dias!diahorahasta3
