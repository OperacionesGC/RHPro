    If Not rsTH.EOF Then
        If Not EsNulo(rsTH!thautpor) Then
            NocAut = rsTH!thautpor
        End If
        If Not EsNulo(rsTH!thdesautpor) Then
            NocNoAut = rsTH!thdesautpor
        End If
        If depurar Then
