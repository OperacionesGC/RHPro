                    Call Cambiar_Horas_Desde(nro_desg, hora_desde_desg, fecha_desde_desg, Cambio_dia)
                    Call Cambiar_Horas_hasta(nro_desg, hora_hasta_desg, fecha_hasta_desg, Cambio_dia)
