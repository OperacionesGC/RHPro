                    End If
                
                
                    Se_Cruza = True
                End If
            End If
            
            'siguiente registracion
            rs.MoveNext
        Loop
        
        'siguiente justificacion
        objRsJustif.MoveNext
    Loop
    
    If Se_Cruza Then
        'Genera Anormalidad
        StrSql = "SELECT * FROM gti_config_tur_hor "
        StrSql = StrSql & " WHERE turnro = " & Nro_Turno
        StrSql = StrSql & " AND conhornro = " & TipoHora
