                             ValorNulo & "," & Empleado.Ternro & "," & Tipo_Hora & "," & Nro_Turno & "," & _
                             Reg_Ent & ",'" & IIf(J = 1, par1, "0000") & "'," & Reg_Sal & ",'" & _
