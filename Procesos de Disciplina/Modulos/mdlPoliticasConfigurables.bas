                hora_entrada2 = "" & objRs!diahoradesde2
                hora_entrada3 = "" & objRs!diahoradesde3
                hora_salida1 = "" & objRs!diahorahasta1
                hora_salida2 = "" & objRs!diahorahasta2
                hora_salida3 = "" & objRs!diahorahasta3
            End If
