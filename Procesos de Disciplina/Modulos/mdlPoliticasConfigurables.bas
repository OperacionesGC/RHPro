Dim Dias As Integer
Dim Horas As Integer
Dim Minutos As Integer
Dim Faltan
Dim Sobran
Dim objRs541 As New ADODB.Recordset
Dim rs_JP As New ADODB.Recordset
Dim Pasa
Dim SegundaParte As Boolean

'Dim EsHoraNormal As Boolean
Dim RestoHsOriginales
Dim SumaA50
Dim SumaA100
'Dim SumaAConvenio
